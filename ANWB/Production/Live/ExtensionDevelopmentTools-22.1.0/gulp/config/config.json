{
    "dbConfig": {
        "keepalive": false,
        "port": 7777,
        "https": false,
        "key": "KEYPEM",
        "cert": "CERTPEM"
    },
    "scaffolding": {
        "keepalive": false,
        "port": 7777
    },
    "fetchConfig": {
        "extension": "AddressBookCustomization,CheckoutCustomization,Columns,HeaderMenuCustomization,HideAddtoCartBtnInGiftItem,MyAccountCustomization,PDPCustomization"
    },
    "extensionMode": true,
    "preserveManifest": false,
    "folders": {
        "source": {
            "source_path": "Workspace",
            "extras_path": "Workspace/Extras"
        },
        "application_manifest": "Workspace/Extras",
        "theme": "Extras",
        "overrides": "Overrides",
        "output": "LocalDistribution/tmp",
        "local": "LocalDistribution",
        "deploy": "DeployDistribution",
        "extensions_dest_name": "Deploy_Extensions",
        "extmech_parent": "-15",
        "extensions_path": [
            "Workspace/AddressBookCustomization",
            "Workspace/CheckoutCustomization",
            "Workspace/HeaderMenuCustomization",
            "Workspace/HideAddtoCartBtnInGiftItem",
            "Workspace/MyAccountCustomization",
            "Workspace/PDPCustomization",
            "Workspace/TrackOrderCustomization"
        ],
        "theme_path": "Workspace/Extras/ANWBTheme",
        "overrides_path": "Workspace/Extras/ANWBTheme/Overrides"
    },
    "application": {
        "application_manifest": {
            "type": "SCA",
            "version": "22.1.2",
            "application": [
                "shopping",
                "myaccount",
                "checkout"
            ],
            "extensible_resources": [
                "skins",
                "javascript",
                "ssp-libraries",
                "sass",
                "templates",
                "services",
                "configuration",
                "assets",
                "suitescript2",
                "translations"
            ]
        }
    },
    "services": {
        "EXTENSION_SERVICE": {
            "script": "customscript_ns_sc_ext_mech_restlets",
            "deploy": "customdeploy_ns_sc_ext_mech_restlets"
        },
        "WEBSITE_SERVICE": {
            "script": "customscript_ns_sc_ext_mech_restlets",
            "deploy": "customdeploy_ns_sc_ext_mech_restlets"
        },
        "FILE_SERVICE": {
            "script": "customscript_ns_sc_ext_mech_restlets",
            "deploy": "customdeploy_ns_sc_ext_mech_restlets"
        },
        "DEPLOY_SERVICE": {
            "script": "customscript_ext_mech_deployer_rest",
            "deploy": "customdeploy_ext_mech_deployer_rest"
        }
    },
    "deploy_config": {
        "update": true,
        "chunk_size": 80
    },
    "credentials": {},
    "nsdeployPath": ".nsdeploy"
}